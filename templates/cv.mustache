                <%/link%>
                <%^link%>
                    \item \bibentry{<%&key%>}
                <%/link%>
            <%/items%>
            \end{itemize}
        <%/posters%>


        %%%%%%%%%%%%
        % PROJECTS %
        %%%%%%%%%%%%

        <%#projects%>
