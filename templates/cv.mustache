            <%#items%>
                <%#link%>
                    \item \href{<%&link%>}{\bibentry{<%&key%>}}
                <%/link%>
                <%^link%>
                    \item \bibentry{<%&key%>}
                <%/link%>
            <%/items%>
            \end{itemize}
        <%/papers%>

        %==== Talks ====%
        <%#talks%>
            \header{Talks}
            \begin{itemize}[leftmargin=0px, label={}, itemsep=\interspace, topsep=0pt]
            <%#items%>
                <%#link%>
                    \item \href{<%&link%>}{\bibentry{<%&key%>}}
                <%/link%>
                <%^link%>
                    \item \bibentry{<%&key%>}
                <%/link%>
            <%/items%>
            \end{itemize}
        <%/talks%>

        %==== Poster Presentations ====%
        <%#posters%>
            \header{Poster Presentations}
            \begin{itemize}[leftmargin=0px, label={}, itemsep=\interspace, topsep=0pt]
            <%#items%>
                <%#link%>
