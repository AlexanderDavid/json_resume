                    <%#positions%>
		    	\textit{<%&title%>}\hfill{}<%&date%>\\
                    <%/positions%>
                <%/position%>
                <%#position%>
                    \textbf{<%&name%>} \hfill <%&location%>\\
		    \textit{<%&position%>}\hfill{}<%&date%>\\
                <%/position%>
		\vspace{\interspace}
            <%/items%>
	    \vspace{-\interspace}
        <%/organisations%>

        %%%%%%%%%%
