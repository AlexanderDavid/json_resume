% DEFINITIONS FOR RESUME %%%%%%%%%%%%%%%%%%%%%%%

% Gobblers
\makeatletter

\newcommand{\checknextargemployer}{\@ifnextchar\bgroup{\gobblenextargemployer}{\end{itemize}} }
\newcommand{\gobblenextargemployer}[1]{ \item #1\@ifnextchar\bgroup{\gobblenextargemployer}{\end{itemize}} }

\newcommand{\checknextargskills}{\@ifnextchar\bgroup{\gobblenextargskills}{\end{tabular}} }
\newcommand{\gobblenextargskills}[2]{ #1 & #2\\ \@ifnextchar\bgroup{\gobblenextargskills}{\end{tabular}} }

\makeatother
% End Gobblers

\newcommand{\lineunder} {
    \vspace*{-8pt} \\
    \hspace*{-18pt} \hrulefill \\
}

\newcommand{\header} [1] {
    {\hspace*{-18pt}\vspace*{6pt} \textsc{#1}}
    \vspace*{-6pt} \lineunder
}

\newcommand{\employer}[5]{%
    \textbf{#1} \hfill #2\\
    \textit{#3} \hfill #4\\
    \vspace{-1.5mm}
    \begin{itemize} \itemsep 0.5pt
	    \item #5
	    \checknextargemployer
    }

\newcommand{\skills}[2]{%
    \vspace{1mm}
    \begin{tabular}{ l l }
	Programming Languages: & #1\\
	Technologies:          & #2\\
    \end{tabular}
    \vspace{1mm}
}

\newcommand{\project} [3] {
    {\textbf{#1}} {\sl #2} \\
    \vspace{-1.5mm}
    \begin{itemize} \itemsep 0.5pt
	    \item #3
	    \checknextargemployer
    }

\newcommand{\contact} [3] {
    \vspace*{-10pt}
    \begin{center}
        {\Huge \scshape {#1}}\\
        #2
    \end{center}
    \vspace*{-8pt}
}

\newcommand{\school} [5] {
    \textbf{#1}\hfill #2\\
    #3 #4 \hfill #5\\
    \vspace{2mm}
}

\newcommand{\award}[2]{
    \textbf{#1}
    \hfill #2
    \vspace*{1mm}\\
}
% END RESUME DEFINITIONS %%%%%%%%%%%%%%%%%%%%%%%

\begin{document}
\vspace*{-40pt}

<%#bio_data%>
    %%%%%%%%%%%%%%%%%%%%%%%
    % CONTACT INFORMATION %
    %%%%%%%%%%%%%%%%%%%%%%%
    \contact{<%&firstname%> <%&familyname%>}
            {<%&location%> $\cdot$ <%&email%> $\cdot$ <%&phone%> $\cdot$ \href{<%&website%>} {\underline{<%&website%>}}}

    %%%%%%%%%%%%%
    % EDUCATION %
    %%%%%%%%%%%%%
    <%#education%>
        \header{Education}
        <%#schools%>
                \school{<%&institution%>}{<%&location%>} {<%&degree%>}{\textit{<%&gpa%>}}{<%&graduation_year%>}
                \vspace{-4px}
        <%/schools%>
    <%/education%>

    %%%%%%%%%%
    % SKILLS %
    %%%%%%%%%%

    <%#skills%>
        \header{Skills}
        \skills <%#details%> {<%#items%><%name%><%^last%>, <%/last%><%/items%>} <%/details%>
    <%/skills%>

    %%%%%%%%%%%%%%
    % EXPERIENCE %
    %%%%%%%%%%%%%%

    <%#experience%>
        \header{Experience}
        <%#items%>
            \employer{<%&organisation%>}{<%&location%>}
            {<%&title%>}{<%&date%>}
            <%#details%>{<%& . %>}<%/details%>
        <%/items%>
    <%/experience%>

    %%%%%%%%%%%%
    % PROJECTS %
    %%%%%%%%%%%%

    <%#projects%>
        \header{Projects}
        <%#items%>
            \project{\href{<%&link%>}{<%&title%>}}{<%&technologies%>}
            <%#details%>{<%& . %>}<%/details%>
        <%/items%>
    <%/projects%>

    %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
    % LEADERSHIP AND INVOLVEMENT %
    %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

    <%#organisations%>
        \header{Leadership and Involvement}
        <%#items%>
            \textbf{<%&name%>} \hfill <%&location%>\\<%&date%>\\
            \vspace{-3.5mm}
            <%#positions%>
                \employer{}{}{<%&title%>}{<%&date%>} <%#details%> {<%& . %>} <%/details%>
                \vspace{-6mm}
            <%/positions%>
        <%/items%>
        \vspace{6mm}
    <%/organisations%>


    %%%%%%%%%%
    % AWARDS %
    %%%%%%%%%%

    <%#awards%>
        \header{Awards}
        <%#items%>
            \award{<%&award%>}{<%&date%>}
        <%/items%>
        <%/awards%>
<%/bio_data%>
\end{document}
